        </tr>
        <tr><td><br></td><td colspan="3"><br></td></tr>
        <tr>
        	  <td></td>
            <td colspan="3">
                <table class="boxSection"><tr><td><div class="textA"><script>document.writeln(lang.LANG_SENSOR_DESC)</script></div></td><tr></table>
            </td>
