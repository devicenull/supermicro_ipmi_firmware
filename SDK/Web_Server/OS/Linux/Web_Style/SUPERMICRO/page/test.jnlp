<jnlp spec="1.0+" codebase="%s">
  <information>
    <title>ATEN Java iKVM Viewer</title>
    <vendor>ATEN</vendor>
    <description>Java Web Start Application</description>
  </information>

  <security>
   <all-permissions/>
  </security>

  <resources>
    <property name="jnlp.packEnabled" value="true"/>
    <j2se version="1.6.0+" initial-heap-size="32M" max-heap-size="128M"/>
    <jar href="%s" download="eager" main="true"/>
  </resources>

  <resources os="Windows" arch="x86">
    <nativelib href="%s" download="eager"/>
  </resources>
  <resources os="Windows" arch="x86_64">
    <nativelib href="%s" download="eager"/>
  </resources>
  <resources os="Windows" arch="amd64">
    <nativelib href="%s" download="eager"/>
  </resources>

  <resources os="Linux" arch="i386">
    <nativelib href="%s" download="eager"/>
  </resources>
  <resources os="Linux" arch="x86">
    <nativelib href="%s" download="eager"/>
  </resources>
  <resources os="Linux" arch="x86_64">
    <nativelib href="%s" download="eager"/>
  </resources>
  <resources os="Linux" arch="amd64">
    <nativelib href="%s" download="eager"/>
  </resources>

  <resources os="Mac OS X" arch="x86_64">
    <nativelib href="%s" download="eager"/>
  </resources>

  <application-desc main-class="tw.com.aten.ikvm.KVMMain">
    <argument>%s</argument>
    <argument>%s</argument>
    <argument>%s</argument>
	<argument>%s</argument>
    <argument>%d</argument>
    <argument>%d</argument>
    <argument>%d</argument>
    <argument>%d</argument>
  </application-desc>
</jnlp>
