// Jack:
// 0: Battery Type Not Present
// 1: Battery Type Present
var SlotStatus = new Array();

function GetStatus(val)
{
	switch (val)
	{
		case 2: 
			return "Battery failed";
		case 3: 
			return "Power failed";
		case 0xb: 
			return "Power AC lost";
		case 1:
			return "Power Supply OK";
		case 4: 
			return "Battery OK";
		case 0xff:
			return "Not present";		
		default:
			return "Error";	
		
	}
}

function GetEngaged(val)
{
	switch (val)
	{
		case 0:
			return " Not engaged"
		case 1: 
			return " Engaged";
		case 2:
			return " Error";	
	}
}

function GetEnable(val)
{
	switch (val)
	{
		case 0:
			return " Disable"
		case 1: 
			return " Enable";
		case 2:
			return " Error";	
	}
}

