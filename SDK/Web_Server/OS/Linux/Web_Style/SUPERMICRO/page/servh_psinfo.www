function powerRefResult(originalRequest) 
{
// 	console.log("powerRefResult()");

 	if (originalRequest.readyState == 4 && originalRequest.status == 200)
	{
	var response = originalRequest.responseText.replace(/^\s+|\s+$/g,"");
	var xmldoc=GetResponseXML(response);
	if(xmldoc == null)
	{
		SessionTimeout();
		return;
	}
	}

	var IPMIRoot=xmldoc.documentElement;//point to IPMI

	var PSInfoObj	=	IPMIRoot.getElementsByTagName("PSInfo");

	PSItemsObj	=	PSInfoObj[0].getElementsByTagName("PSItem");

	for(i=0;i<4;++i)
	{
		PSItems[i][1][0].innerHTML  = "Status";
		PSItems[i][1][1].innerHTML  = GetStatus(parseInt( PSItemsObj[i].getAttribute("a_b_PS_Status_I2C"),16));
		SlotStatus[i] = 0;

		if(parseInt( PSItemsObj[i].getAttribute("a_b_PS_Status_I2C"),16)!=0xff)
		{
			
			// 0 = power supply
			if(parseInt( PSItemsObj[i].getAttribute("at_b_PSType"),16)==0)
			{

				PSItems[i][1][2].innerHTML  = "AC Input Voltage";

				PSItems[i][1][3].innerHTML  = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_1_0"),16)+" (V)";

				PSItems[i][1][4].innerHTML  = "AC Input Current";
				PSItems[i][1][5].innerHTML  = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_2_0"),16) / 100 +" (A)";

				PSItems[i][1][6].innerHTML  = "DC 12V Output Voltage";
				PSItems[i][1][7].innerHTML  = (parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_3_0"),16) + parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_3_1"),16)*255) / 100 + " (V)";

				PSItems[i][1][8].innerHTML  = "DC 12V Output Current";
				PSItems[i][1][9].innerHTML  = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_4_0"),16) / 10 +" (A)";

				c = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_5_0"),16);
				f = 9 * c / 5+ 32;
				PSItems[i][1][10].innerHTML  = "Temperature 1";
				PSItems[i][1][11].innerHTML  = (c + " C/ " + f + " F");

				c = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_6_0"),16);
				f = 9 * c / 5+ 32;

				PSItems[i][1][12].innerHTML  = "Temperature 2";
				PSItems[i][1][13].innerHTML  = (c + " C/ " + f + " F");

				PSItems[i][1][14].innerHTML  = "Fan 1";
				PSItems[i][1][15].innerHTML  = (parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_7_0"),16) + parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_7_1"),16)*255) * 100 +" RPM";

				// PSItems[i][1][15].innerHTML  = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_7_0"),16)+" RPM";

				PSItems[i][1][16].innerHTML  = "Fan 2";
				PSItems[i][1][17].innerHTML  = (parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_8_0"),16)+ parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_8_1"),16)*255) * 100 +" RPM";
				// PSItems[i][1][17].innerHTML  = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_8_0"),16)+" RPM";

				PSItems[i][1][18].innerHTML  = "DC 12V Output Power";
				PSItems[i][1][19].innerHTML  = (parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_9_0"),16) + parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_9_1"),16) * 255) + " (W)"

				PSItems[i][1][20].innerHTML  = "AC Input Power";
				PSItems[i][1][21].innerHTML  = (parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_a_0"),16) + parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_a_1"),16) * 255) + " (W)"

				PSItems[i][1][22].innerHTML  = "PWS Serial Number";
				PSItems[i][1][23].innerHTML  = PSItemsObj[i].getAttribute("at_bb_PSInfoName");


			}else
			{
				SlotStatus[i] = 1;
				PSItems[i][1][2].innerHTML  = "Temperature";
				test = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_1_0"),16) + parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_1_1"),16) * 256;
				PSItems[i][1][3].innerHTML  = parseInt( test)+" (C)";

				PSItems[i][1][4].innerHTML  = "Remaining Energy";
				PSItems[i][1][5].innerHTML  = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_2_0"),16)+" (%)";

				PSItems[i][1][6].innerHTML  = "Voltage";
				test = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_3_0"),16) + parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_3_1"),16) * 256;
				PSItems[i][1][7].innerHTML  = parseInt( test)+" (mV)";
// Jack: 
// Correction: Current is in 2's 
//
				PSItems[i][1][8].innerHTML  = "Current";
				test = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_4_0"),16) + parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_4_1"),16) * 256;
		  
				if((test & 0x8000) == 0x8000)
				{
				  test = ( (0xFFFF - test) + 1);
					PSItems[i][1][9].innerHTML  = "-" + ( test * 10 ) +" (mA)";
				}else
				{
				  PSItems[i][1][9].innerHTML  = ( test * 10 ) +" (mA)";
				}

 				console.log("after current value test=%d", test);

				// PSItems[i][1][9].innerHTML  = parseInt( test) * 10 +" (mA)";

				PSItems[i][1][10].innerHTML  = "Cycle Count";
				test = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_5_0"),16) + parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_5_1"),16) * 256;
				PSItems[i][1][11].innerHTML  = parseInt( test) * 10 +" (Times)";

				test = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_6_0"),16);
				test1 = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_7_0"),16);
				var test_ret, test1_ret;
				
				PSItems[i][1][12].innerHTML  = "Auto Discharge Timer";

				if((test & 0x80) == 0x80)
					test_ret = 1;
				else
					test_ret = 0;

				if((test1 & 0x80) == 0x80)
					test1_ret = 1;
				else
					test1_ret = 0;

				PSItems[i][1][13].innerHTML = GetEnable(test1_ret)+" , " + GetEngaged(test_ret);


				PSItems[i][1][14].innerHTML  = "Manual Discharge";

				if((test & 0x40) == 0x80)
					test_ret = 1;
				else
					test_ret = 0;

				if((test1 & 0x40) == 0x80)
					test1_ret = 1;
				else
					test1_ret = 0;

				PSItems[i][1][15].innerHTML = GetEnable(test1_ret)+" , " + GetEngaged(test_ret);


				PSItems[i][1][16].innerHTML  = "Remaining days before auto-discharge";
				test = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_6_0"),16);
				PSItems[i][1][17].innerHTML  = parseInt(test & 0x3F)+ " Days ( Default:30 , Max:63 )";

				PSItems[i][1][18].innerHTML  = "Auto Timer Setting";
				test = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_7_0"),16);
				PSItems[i][1][19].innerHTML  = parseInt(test & 0x3F)+ " Days";

			}//if(parseInt( PSItemsObj[i].getAttribute("at_b_PSType"),16)==0)
		}//if(parseInt( PSItemsObj[i].getAttribute("a_b_PS_Status_I2C"),16)!=0xff)
		
		//Setup the bbp setting block

		//
		// Check if PSS ITEM is Battery or Power Supply
		//
		BBPSettingItems[i][1][0].innerHTML  = "Audio Discharge Timer "+ i;
		BBPSettingItems[i][1][1].innerHTML  = "Manual Discharge "+ i

		if(parseInt( PSItemsObj[i].getAttribute("at_b_PSType"),16)==1)
		{
			test1 = parseInt( PSItemsObj[i].getAttribute("at_bb_PSInfoValue_7_0"),16);

// 			console.log("i = " + i + " at_bb_PSInfoValue_7_0 value = " + test1);

			if((test1 & 0x80) == 0x80)
			{
				BBPSettingItems[i][1][2].checked =true;
				BBPSettingItems[i][1][3].checked =false;
			}else
			{
				BBPSettingItems[i][1][2].checked =false;
				BBPSettingItems[i][1][3].checked =true;
			}

			if((test1 & 0x40) == 0x40)
			{
				BBPSettingItems[i][1][4].checked =true;
				BBPSettingItems[i][1][5].checked =false;
			}else
			{
				BBPSettingItems[i][1][4].checked =false;
				BBPSettingItems[i][1][5].checked =true;
 			}


			var var_min_dd = 0;
			var var_max_dd = 63;
			var var_num;
			var var_index;
			var tmp_opt_obj;

			for(var_num = var_min_dd; var_num <= var_max_dd; var_num++)
			{
				if(browser_ie) var_index = var_num - var_min_dd;
				else var_index = null;
				tmp_opt_obj = new Option(var_num, var_num);

				BBPSettingItems[i][j][6].add(tmp_opt_obj, var_index);
			}

			BBPSettingItems[i][1][6].value = parseInt(test1 & 0x3F);
			
		}
		else
		{

		  BBPSettingItems[i][1][0].disabled = true;
		  BBPSettingItems[i][1][1].disabled = true;

		  BBPSettingItems[i][1][2].checked = false;
		  BBPSettingItems[i][1][2].disabled = true;

		  BBPSettingItems[i][1][3].checked =false;
		  BBPSettingItems[i][1][3].disabled = true;

		  BBPSettingItems[i][1][4].checked =false;
		  BBPSettingItems[i][1][4].disabled = true;

		  BBPSettingItems[i][1][5].checked =false;
		  BBPSettingItems[i][1][5].disabled = true;
		  

		  // Select
		  BBPSettingItems[i][1][6].disabled = true;
		}
	}//for(i=0;i<4;++i)



		//Setup the bbp setting block

		BBPTimeoutItem[1][0].innerHTML  = "Timeout Value for graceful shutdown";
		BBPTimeoutItem[1][1].innerHTML  = " (Seconds)";

		if(parseInt( PSInfoObj[0].getAttribute("at_b_PSTimeoutEnable"),16)==1)
		{
			BBPTimeoutItem[1][2].checked =true;
			BBPTimeoutItem[1][3].checked =false;
		}else
		{
			BBPTimeoutItem[1][2].checked =false;
			BBPTimeoutItem[1][3].checked =true;
		}

		BBPTimeoutItem[1][4].value = parseInt( PSInfoObj[0].getAttribute("at_w_PSTimeoutValue"),16);
		BBPTimeoutItem[1][6].innerHTML =parseInt( PSInfoObj[0].getAttribute("BBP_TIMEOUT_VALUE"),16) + " sec";
		

		document.getElementById(BBPTimeoutItem[0][5]).setAttribute("value", lang.LANG_CONF_NETWORK_SAVE);

// 		console.groupEnd();
}

